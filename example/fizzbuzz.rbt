def FizzBuzz(n)
	if n%15 == 0 then
		return "FizzBuzz"
	elsif n%5 == 0 then
		return "Fizz"
	elsif n%3 == 0 then
		return "Buzz"
	else
		return n;
	end
end
	
i = 1
while i <= 100 do
	puts( FizzBuzz(i) )
	i += 1
end

